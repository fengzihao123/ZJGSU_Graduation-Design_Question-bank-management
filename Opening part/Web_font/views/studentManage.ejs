<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Document</title>
    <link rel='stylesheet' href='/iconfont/iconfont.css'/>
    <link rel='stylesheet' href='/stylesheets/bootstrap.min.css'/>
    <link rel='stylesheet' href='/stylesheets/studentManage.css'/>
</head>
<body>
    <div class="oneSelect">
        <% collectList.forEach(function (item){ %>
            <form action="/changeIndex" method="post" class="oneSelect">
                <input type="submit" value="<%= collectList.indexOf(item) + 1%>" class="submitInput" name="index">
            </form>
        <% }) %>
    </div>
    <text><%= collectList[index].id%></text>


</body>
</html>