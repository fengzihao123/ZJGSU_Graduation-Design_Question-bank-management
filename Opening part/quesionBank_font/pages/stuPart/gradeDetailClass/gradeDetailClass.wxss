.my_chart{
    width: 750rpx;
    height: 500rpx;
}
.ec-canvas{
    width: 100%;
    height: 100%;
}
.totalGradeContainer{
    color: #6666FF;
    display: block;
    margin-left: 40rpx;
    font-weight: 600;
    margin-top: 80rpx;
    margin-bottom: 100rpx;
}
.totalGradeContainer1{
    display: block;
    margin-left: 40rpx;
    font-weight: 600;
    color: red;
}
.oneQuestionContainer{
    display: flex;
    margin-top: 20rpx;
    margin-left: 40rpx;
}
.index{
    display: grid;
}
.title{
    font-weight: 600;
    font-size: 30rpx;
    margin-right: 50rpx;
}
.title_stem{
    font-weight: 600;
    font-size: 30rpx;
    margin-left: 120rpx;
}
.content_id{
    width: 20rpx;
    margin-top: 10rpx;
}
.content{
    margin-top: 10rpx;
    margin-left: 80rpx;
}
.content_stem{
    margin-top: 10rpx;
    display: block;
    width: 300rpx;
    margin-left: 80rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}
.queType{
    display: grid;
}
.stem{
    display: grid;
}
.errorCount{
    display: grid;
}
.title_error{
    font-weight: 600;
    font-size: 30rpx;
    margin-left: 190rpx;
}
.content_error{
    margin-top: 10rpx;
    margin-left: 80rpx;
}